'Date: 08/08/2019
'Script: ex11 demonstrate String Replace
'##############################################

dim Message1 as string;


'Create a message to the user
Message1 = "Inlet Discharge Pressure Input";

Message1 = StringReplace(Message1,"In","Out",0,-1,0);
LogMessage(Message1);

MasterObject.Trigger11 = false;
