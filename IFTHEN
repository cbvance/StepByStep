'###############################
'Author: Charles Vance
'Date: 08/08/2019
'Script: ex1 demonstrate IF-THEN
'###############################

'dimension variables
dim a as boolean;
dim b as integer;

'assign values from external reference
a = Me.boolTest;

'if variable a is true execute
if a then 
   b = 3; 'set internal variable to 3
endif;

'assign external uda to internal variable
Me.integerTest = b;

'end script execution 
Me.udaTrigger = false;
